generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SosEvent {
  id           String    @id @default(uuid())
  stationId    String
  stationName  String
  status       String    @default("pending") // pending, accepted, ended
  acceptedBy   String?
  acceptedAt   DateTime?
  createdAt    DateTime  @default(now())
  endedAt      DateTime?

  @@map("sos_events")
}